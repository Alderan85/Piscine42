#include <stdio.h>
#include <stdlib.h>

int    *ft_range(int min, int max)
{
    int *range;
    int count;
    int i;
    
    if(min > max)
        return(0);
        
    count = min;
    i = max - min;
    range = (int *)malloc(i * sizeof(int));    
    if (!range)
	{
		return (0);
	}
    while(count <= i)
    {
        range[count] = min + count;
        count++;
    }
    return(range);
}



int main()
{
    int min;
    int max;
    int *range;
    
    min = -5;
    max = 10;
    
    range = ft_range(min, max);
    printf("%d\n", range[15]);
    
    return (0);
}
