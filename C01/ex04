#include <unistd.h>
//#include <stdio.h>

void ft_ultimate_div_mod(int *a, int *b)
{
    int res;
    int rem;
    
    res = *a / *b;
    rem = *a % *b;
    
    *a = res;
    *b = rem;
    
    //printf("Div Result : %d\n", *a);
    //printf("Div Result : %d\n", *b);
    
    
}

    
