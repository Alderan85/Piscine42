#include <unistd.h>
//// REMOOOOVEEEEEE
#include <stdio.h>

char *ft_strcpy(char *dest, char *src)
{
    int i;    
    i = 0;
    
    while(*src)
    {
        dest[i] = src[i];
        i++;
    }    
    // To add null at the end    
    dest[i + 1] = '\0';
    
    return(dest);    
}

///// REMOOOVVEEEEEE
int main() 
{
    
    char testString[7] = "banana";
    char destString[7] = "";
    
    //ft_strcpy(destString, testString);
    printf("%s\n",ft_strcpy(destString,testString));
    return 0;
}
