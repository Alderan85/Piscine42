#include <unistd.h>
#include <stdio.h>

char	*ft_strstr(char *str, char *to_find)
{
	int strtofindlen;
	int i;
	int i2;

	strtofindlen = 0;
	i = 0;
	i2 = 0;
	while (to_find[strtofindlen])
	{
		strtofindlen++;
	}
	if (strtofindlen == 0)
		return (str);
	while (str[i])
	{
		while (to_find[i2] == str[i + i2])
		{
			if (i2 + 1 == strtofindlen)
				return (str + i);
			i2++;
		}
		i2 = 0;
		i++;
	}
	return (NULL);
}

int	main()
{
	char str[] = "abcDEFghijklmDEFnopqrstuvwxyz";
	char strtofind[] = "DEF";
	printf("%s", ft_strstr(str, strtofind));
	return (0);
}
